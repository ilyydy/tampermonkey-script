{
  "name": "douban-bookshelf",
  "private": true,
  "version": "0.0.4",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "pnpm type-check && pnpm build-only",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "coverage": "vitest run --coverage",
    "changelog": "conventional-changelog -p angular -r 0 -t douban-bookshelf@ --commit-path $PWD --pkg package.json -i CHANGELOG.md -s",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "lint:check": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts"
  },
  "packageManager": "pnpm@9.1.2",
  "dependencies": {
    "@element-plus/icons-vue": "^2.0.10",
    "csv-stringify": "^6.2.3",
    "element-plus": "^2.2.19",
    "vue": "3.4.27",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "5.0.4",
    "conventional-changelog-cli": "5.0.0",
    "tsx": "4.11.0",
    "typescript": "5.4.5",
    "vite": "5.2.12",
    "vitest": "1.6.0",
    "vue-tsc": "2.0.19"
  }
}