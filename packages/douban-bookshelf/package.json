{
  "name": "douban-bookshelf",
  "private": true,
  "version": "0.0.4",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "pnpm type-check && pnpm build-only",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "coverage": "vitest run --coverage",
    "changelog": "conventional-changelog -p angular -r 0 -t douban-bookshelf@ --commit-path $PWD --pkg package.json -i CHANGELOG.md -s",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "lint:check": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts"
  },
  "packageManager": "^pnpm@8.6.6",
  "dependencies": {
    "@element-plus/icons-vue": "^2.0.10",
    "csv-stringify": "^6.2.3",
    "element-plus": "^2.2.19",
    "vue": "3.3.4",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "4.2.3",
    "conventional-changelog-cli": "3.0.0",
    "tsx": "3.12.7",
    "typescript": "5.1.6",
    "vite": "4.3.9",
    "vitest": "0.31.1",
    "vue-tsc": "1.8.4"
  }
}